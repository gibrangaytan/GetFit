<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.utep.cs.cs4390.getfit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.utep.cs.cs4390.getfit.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="BodyParts"
            android:name=".BodyParts" >
            <intent-filter >
                <action android:name="edu.utep.cs.cs4390.getfit.BodyParts"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:label="ExerciseList"
            android:name=".ExerciseList" > 
            <intent-filter >
                <action android:name="edu.utep.cs.cs4390.getfit.ExerciseList"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="ExerciseView"
            android:name=".ExerciseView" > 
            <intent-filter >
                <action android:name="edu.utep.cs.cs4390.getfit.ExerciseView"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="Profiles"
            android:name=".Profiles" > 
            <intent-filter >
                <action android:name="edu.utep.cs.cs4390.getfit.Profiles"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
